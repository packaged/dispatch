{
  "name": "packaged/dispatch",
  "description": "Asset Management Middleware for Stack PHP",
  "license": "MIT",
  "authors": [
    {
      "name": "Brooke Bryan",
      "email": "brooke@bajb.net"
    }
  ],
  "require": {
    "php": ">=7.1",
    "packaged/helpers": "^1.0||^2.0",
    "packaged/http": "~1.1",
    "tedivm/jshrink": "~1.3"
  },
  "suggest": {
    "ext-apcu": "*"
  },
  "require-dev": {
    "ext-apcu": "*",
    "phpunit/phpunit": "7.5.*"
  },
  "autoload": {
    "psr-4": {
      "Packaged\\Dispatch\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Packaged\\Dispatch\\Tests\\TestComponents\\": "tests/_root/src/TestComponents",
      "Packaged\\Dispatch\\Tests\\": "tests"
    }
  }
}
